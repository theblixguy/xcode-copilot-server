{
  "name": "xcode-copilot-server",
  "version": "3.2.0",
  "description": "OpenAI-compatible proxy API server for Xcode, powered by GitHub Copilot",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theblixguy/xcode-copilot-server.git"
  },
  "engines": {
    "node": "25.6.0"
  },
  "bin": {
    "xcode-copilot-server": "dist/index.js"
  },
  "files": [
    "dist/",
    "config.json5",
    "scripts/"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.check.json",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@fastify/cors": "11.2.0",
    "@github/copilot-sdk": "0.1.24",
    "commander": "14.0.3",
    "fastify": "5.7.4",
    "json5": "2.2.3",
    "koffi": "2.15.1",
    "picocolors": "1.1.1",
    "plist": "3.1.0",
    "tokenx": "1.3.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "10.0.1",
    "@types/node": "25.3.0",
    "@types/plist": "^3.0.5",
    "eslint": "10.0.0",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.0",
    "vitest": "4.0.18"
  }
}
